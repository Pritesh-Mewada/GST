<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="com.graphysis.gst.Developers"
    >
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:scaleType="fitXY"
            android:src="@drawable/bubbles"
            android:layout_gravity="bottom"
            />
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center_vertical">
            <android.support.v7.widget.Toolbar
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@drawable/blue_gradient"
                android:id="@+id/toolbar"
                android:elevation="4dp"
                app:titleTextColor="#ffffff"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                ></android.support.v7.widget.Toolbar>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_horizontal"
                >
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/pritesh_icon"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pritesh Mewada"
                        android:textColor="#e21a53"
                        android:textSize="24sp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Btech Extc VJTI"
                        android:textColor="#5d5d5d"
                        android:textSize="16sp"
                        />
                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_marginLeft="30dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:clipToPadding="false"
                        android:clipChildren="false"
                        android:paddingBottom="5dp"
                        >
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false"
                            android:clipChildren="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pritesh_call"
                                android:translationZ="10dp"
                                android:src="@drawable/call_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pritesh_email"
                                android:translationZ="10dp"
                                android:src="@drawable/mail_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pritesh_linked"
                                android:translationZ="10dp"
                                android:src="@drawable/linkedin_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_horizontal"
                >
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/kush_icon"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="5dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kush Gadhvi"
                        android:textColor="#e21a53"
                        android:textSize="24sp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Btech Extc VJTI"
                        android:textColor="#5d5d5d"
                        android:textSize="16sp"
                        />
                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_marginLeft="30dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:clipToPadding="false"
                        android:clipChildren="false"
                        android:paddingBottom="5dp">
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/kush_call"
                                android:translationZ="10dp"
                                android:src="@drawable/call_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/kush_email"
                                android:translationZ="10dp"
                                android:src="@drawable/mail_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/kush_linked"
                                android:translationZ="10dp"
                                android:src="@drawable/linkedin_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_horizontal"
                >
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/pavan_icon"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pavan Suthar"
                        android:textColor="#e21a53"
                        android:textSize="24sp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Graphic Designer"
                        android:textColor="#5d5d5d"
                        android:textSize="16sp"
                        />
                    <LinearLayout
                        android:layout_width="200dp"
                        android:layout_marginLeft="30dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:clipToPadding="false"
                        android:clipChildren="false"
                        android:paddingBottom="5dp">
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pavan_call"
                                android:translationZ="10dp"
                                android:src="@drawable/call_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pavan_email"
                                android:translationZ="10dp"
                                android:src="@drawable/mail_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:padding="10dp"
                            android:clipToPadding="false">
                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:id="@+id/pavan_instagram"
                                android:translationZ="10dp"
                                android:src="@drawable/instagram_icon"
                                android:scaleType="fitXY"
                                android:background="@drawable/round"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:elevation="6dp"
                                />
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</FrameLayout>
